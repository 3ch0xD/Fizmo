{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/main.py"
      }
    ],
    "env": {
      "DATABASE_URL": "sqlite:///instance/database.db"
    },
    "gitignore": [
      ".venv",
      "instance/database.db"
    ]
  }